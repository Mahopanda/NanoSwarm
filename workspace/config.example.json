{
  "agents": {
    "defaults": {
      "model": "gemini/gemini-3-flash-preview",
      "workspace": "~/.nanoswarm/workspace"
    }
  },
  "providers": {
    "gemini": {
      "apiKey": "YOUR_GEMINI_API_KEY"
    }
  },
  "server": {
    "port": 4000,
    "host": "0.0.0.0",
    "name": "NanoSwarm"
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "${TELEGRAM_BOT_TOKEN}",
      "allowFrom": [],
      "adminUsers": [],
      "proxy": "",
      "replyToMessage": false,
      "sttProvider": "groq",
      "sttApiKey": "${GROQ_API_KEY}",
      "group": {
        "requireMention": true,
        "policy": "open",
        "cooldownSeconds": 0
      },
      "accounts": {
        "finance": {
          "token": "${TELEGRAM_FINANCE_BOT_TOKEN}",
          "boundAgent": "finance-agent"
        }
      }
    }
  }
}
